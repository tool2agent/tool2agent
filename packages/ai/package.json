{
  "name": "@tool2agent/ai",
  "version": "0.0.1",
  "description": "tool2agent bindings for AI SDK",
  "type": "module",
  "main": "dist-cjs/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist-cjs/index.js",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "dist-cjs",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc && tsc -p tsconfig.cjs.json",
    "build:all": "tsc -p tsconfig-all.json",
    "build:watch": "tsc --watch",
    "clean": "rm -rf dist dist-cjs",
    "test": "pnpm test:unit && pnpm test:types && pnpm test:runtime",
    "test:runtime": "mocha --config .mocharc.runtime.json",
    "test:unit": "mocha --config .mocharc.unit.json",
    "test:types": "tsc --project test-d/tsconfig.json",
    "test:watch": "mocha --config .mocharc.runtime.json --watch",
    "prepack": "pnpm run build",
    "example:censorship-bypass": "pnpx tsx examples/censorship-bypass.ts",
    "example:middleware": "pnpx tsx examples/middleware.ts",
    "example:agent-consensus": "pnpx tsx examples/agent-consensus.ts",
    "example:airline-booking-chat": "pnpx tsx examples/airline-booking-chat.ts"
  },
  "keywords": [
    "tool2agent",
    "ai-sdk",
    "llm",
    "ai"
  ],
  "author": "klntsky",
  "license": "Unlicense",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/promptware/tool2agent.git"
  },
  "homepage": "https://github.com/promptware/tool2agent#readme",
  "bugs": {
    "url": "https://github.com/promptware/tool2agent/issues"
  },
  "readme": "README.md",
  "devDependencies": {
    "@types/chai": "^5.2.2",
    "@types/lodash": "^4.17.16",
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.9.2",
    "chai": "^5.2.0",
    "dotenv": "^17.2.3",
    "lodash": "^4.17.21",
    "mocha": "^11.5.0",
    "mocha-suppress-logs": "^0.6.0",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "typescript": "^5.9.3",
    "zod": "^4.1.12"
  },
  "dependencies": {
    "@tool2agent/types": "workspace:*",
    "@ai-sdk/provider-utils": "^3.0.13",
    "@openrouter/ai-sdk-provider": "^1.2.0",
    "@types/deep-equal": "^1.0.4",
    "ai": "^5.0.81",
    "deep-equal": "^2.2.3",
    "immer": "^10.1.1",
    "ts-pattern": "^5.7.1",
    "util": "^0.12.5"
  },
  "peerDependencies": {
    "zod": "^4"
  }
}
