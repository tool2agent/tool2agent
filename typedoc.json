{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["packages/ai", "packages/types", "packages/schemas"],
  "entryPointStrategy": "packages",
  "out": "docs",
  "tsconfig": "tsconfig.docs.json",
  "exclude": [
    "**/*.test.ts",
    "**/*.test-d.ts",
    "**/test/**",
    "**/test-d/**",
    "**/examples/**",
    "**/node_modules/**",
    "**/dist/**",
    "**/dist-cjs/**"
  ],
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeInternal": true,
  "readme": "none",
  "name": "tool2agent API Documentation",
  "theme": "default",
  "includeVersion": true,
  "gitRevision": "main",
  "skipErrorChecking": true
}
